{
  "manifest_version": 3,
  "name": "TS Popup Only",
  "version": "0.1.0",
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "128": "icons/icon128.png"
  },
  "permissions": ["activeTab", "scripting", "storage"],          
  "host_permissions": ["http://*/*", "https://*/*"]   
}
